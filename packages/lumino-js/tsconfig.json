{
    "compilerOptions": {
        "target": "es2020",
        "allowJs": false,
        "sourceMap": true,
        "removeComments": false,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "strictNullChecks": false,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "baseUrl": "./"
    }
}
